<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingTop="8dp"
    tools:context=".MainActivity"
    tools:ignore="HardcodedText">

    <Button
        android:id="@+id/buttonLaunch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Run launch coroutines" />

    <Button
        android:id="@+id/buttonLaunchJoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Run launch join coroutines" />

    <Button
        android:id="@+id/buttonLaunchJoinMulti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Run launch Multi join coroutines" />

    <Button
        android:id="@+id/buttonOne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Run one coroutine" />

    <Button
        android:id="@+id/buttonAsync"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Run async coroutine" />

    <Button
        android:id="@+id/buttonAsyncMulti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Run async multi coroutine" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/listUsers"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>
